{
    "name": "indev/adcconsole",
    "description": "Console for Adianti Framework Community Edition",
    "type": "library",
    "license": "Apache-2.0",
    "version": "1.0.1",
    "scripts": {
            "post-root-package-install": [
             "@php -r \"copy('bin/adconsole', 'adconsole');\""
            ]
    },
    "authors": [
        {
            "name": "Alexandre Souza",
            "email": "alexandre@indev.net.br"
        }
    ],
      "support": {
    "email": "team@indev.net.br"
  },
    "minimum-stability": "stable",

    "autoload": {
        "psr-4": {
             "App\\Command\\": "src/",
             "Indev\\AdConsole\\Test\\": "test/",
             "Indev\\Command\\Bin\\": "bin/"

        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/phpunit-bridge": "*"
    },
    "require": {
        "symfony/console": "^4.1",
        "robmorgan/phinx": "^0.10.6",
        "rbdwllr/reallysimplejwt": "^1.0",
        "ext-json": "*"
    }
}
