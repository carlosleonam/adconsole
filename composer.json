{
    "name": "indev/adcconsole",
    "description": "Console for Adianti Framework Community Edition",
    "type": "library",
    "license": "Apache-2.0",
    "version": "0.2",
    "authors": [
        {
            "name": "Alexandre Souza",
            "email": "alexandre@indev.net.br"
        }
    ],
      "support": {
    "email": "team@indev.net.br"
  },

  "scripts": {
    "post-install-cmd": [
      "php -r \"copy('bin/console', 'vendor/bin/adconsole');\""
      ]
  },


    "minimum-stability": "stable",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "App\\Command\\": "src/",
             "Indev\\AdConsole\\Test\\": "test/",
              "Indev\\Command\\Bin\\": "bin/"

        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/phpunit-bridge": "*"
    },
    "require": {
        "symfony/console": "^4.1",
        "robmorgan/phinx": "^0.10.6"
    },
  "bin": ["bin/adconsole"]
}
